    server {
        listen 127.0.0.1:@AURA_USER_PORT8@;
#ifdef NGX_MAINDOMAIN
        server_name @NGX_ROOTDOMAIN@;
#else
        server_name @NGX_SUBDOMAIN@.@NGX_ROOTDOMAIN@;
#endif
#ifdef NGX_OVERRIDE_SUBDOMAIN
        root /aura/accounts/@AURA_USER_CLASS@/@AURA_USER_NAME@/sites/@NGX_ROOTDOMAIN@/@NGX_OVERRIDE_SUBDOMAIN@;
#else
        root /aura/accounts/@AURA_USER_CLASS@/@AURA_USER_NAME@/sites/@NGX_ROOTDOMAIN@/@NGX_SUBDOMAIN@;
#endif
        index index.htm index.html index.php;
#ifdef NGX_ENABLE_AUTOINDEX
        autoindex on;
#else
        autoindex off;
#endif
        access_log off;
        absolute_redirect off;

#ifdef NGX_INCLUDE
#include @NGX_INCLUDE@
#endif        

        location @404 { return 404; }
        location / {
#if defined(NGX_ENABLE_CMSMS) || defined(NGX_ENABLE_WORDPRESS)
            try_files $uri $uri/ /index.php?page=$request_uri;
#elif NGX_ENABLE_ENANO
            try_files $uri $uri/ @enano-rewrite;
#elif NGX_ENABLE_PHOEBUS
            try_files $uri $uri/ @phoebus-rewrite;
#else
            try_files $uri/ $uri =404;
#endif
        }

#ifdef NGX_ENABLE_ENANO
        location @enano-rewrite {
            rewrite ^/(.*)$ /index.php?title=$1&$args last;
        }
#endif

#ifdef NGX_ENABLE_PHOEBUS
        location @phoebus-rewrite {
            rewrite ^ /index.php?component=site&path=$uri last;
        }
#endif

#ifdef NGX_ENABLE_PHP
        location ~ \.php$ {
            include /aura/configuration/system/nginx/standard-php.inc;
#ifndef NGX_ENABLE_PHP_404
            fastcgi_intercept_errors on;
            fastcgi_buffering off;
            error_page 404 = @404;
#endif
#ifdef PHP_VERSION_56
            fastcgi_pass unix:/var/run/aura-@AURA_USER_CLASS@-@AURA_USER_NAME@-php56.socket;
#elif PHP_VERSION_70
            fastcgi_pass 127.0.0.1:@AURA_USER_PORT9@;
#else
            fastcgi_pass unix:/var/run/aura-@AURA_USER_CLASS@-@AURA_USER_NAME@-php.socket;
#endif
        }
#endif
        add_header X-BinOC socket=true always;
    }